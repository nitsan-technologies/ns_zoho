{
    "name": "nitsan/ns-zoho",
    "type": "typo3-cms-extension",
    "description": "Easily install and configure your typo3 form with zoho CRM",
    "homepage": "https://extensions.typo3.org/extension/ns_zoho/",
    "license": "GPL-2.0+",
    "keywords": [
        "TYPO3 CMS",
        "Zoho",
        "Nitsan",
        "Form"
    ],
    "support": {
        "issues": "https://github.com/nitsan-technologies/ns_zoho/issues"
    },
    "require": {
        "typo3/cms-core": "^10  || ^11 || ^12"
    },
    "replace": {
        "typo3-ter/ns-zoho": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Nitsan\\NsZoho\\": "Classes"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_zoho"
        }
    }
}