{
    "name": "nitsan/ns-zoho",
    "type": "typo3-cms-extension",
    "description": "The TYPO3 Zoho Extension offers seamless integration between TYPO3 forms and Zoho CRM, streamlining the lead management process. By automatically capturing leads from your website, this innovative integration ensures no opportunity is missed.",
    "homepage": "https://demo.t3planet.com/t3-extensions/typo3-zoho-crm",
    "license": "GPL-2.0+",
    "keywords": [
        "TYPO3 CMS",
        "Zoho",
        "Nitsan",
        "Form"
    ],
    "support": {
        "issues": "https://github.com/nitsan-technologies/ns_zoho/issues"
    },
    "require": {
        "typo3/cms-core": "^10  || ^11 || ^12 || ^13"
    },
    "replace": {
        "typo3-ter/ns-zoho": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Nitsan\\NsZoho\\": "Classes"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_zoho"
        }
    }
}
